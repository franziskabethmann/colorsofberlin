<div class="container">
  <h1>Add a Picture:</h1>
  <%= simple_form_for [@picture] do |f| %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.input :date %>
    <%= f.input :address %>
    <%= f.submit "Add Picture", class: "btn btn-primary" %>
  <% end %>
  <p><%= link_to "Back to Picture overview", pictures_path %></p>
</div>
